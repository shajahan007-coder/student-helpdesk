{
  "routes": [
    {
      "src": "/auth/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/tickets/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/createTicket",
      "dest": "/index.js",
      "methods": ["POST"]
    },
    {
      "src": "/",
      "dest": "/index.js",
      "methods": ["GET"]
    }
  ]
}